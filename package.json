{
    "name": "vue-boilerplate",
    "version": "0.1.0",
    "scripts": {
        "serve": "cross-env port=9003 build/bin/dev-server",
        "build-dll": "webpack -p --progress --config build/webpack.dll.config.js",
        "build-dev": "cross-env NODE_ENV=development npm run build",
        "build-prod": "cross-env NODE_ENV=production npm run build",
        "build-analyze": "cross-env NODE_ENV=production IS_ANALYZE=true npm run build",
        "build": "cross-env baseUrl=/vue-boilerplate/dist/ build/bin/production"
    },
    "webpackConfig": {
        "externals": {},
        "transpileModules": [
            "muse-ui"
        ],
        "webpackDefine": {}
    },
    "devServerConfig": {},
    "dependencies": {
        "@babel/polyfill": "^7.4.4",
        "animate.css": "^3.6.1",
        "axios": "^0.21.1",
        "crypto-js": "^4.1.1",
        "moment": "^2.27.0",
        "muse-ui": "^3.0.2",
        "muse-ui-message": "^0.2.1",
        "muse-ui-toast": "^0.3.0",
        "numbro": "^2.0.6",
        "throttle-debounce": "^2.0.1",
        "vue": "~2.6.10",
        "vue-class-component": "^6.2.0",
        "vue-property-decorator": "^8.0.0",
        "vue-router": "^3.0.1",
        "vue-star-rating": "^1.5.1",
        "vuex": "^3.0.1",
        "vuex-class": "^0.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.4.4",
        "@babel/polyfill": "^7.4.4",
        "@babel/preset-env": "^7.4.4",
