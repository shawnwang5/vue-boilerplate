        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "terser-webpack-plugin": "^3.0.1",
        "ts-loader": "^5.4.4",
        "typescript": "^3.4.5",
        "vue-loader": "^15.9.2",
        "vue-template-compiler": "^2.6.10",
        "webpack": "^4.30.0",
        "webpack-bundle-analyzer": "^3.3.2",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.8.0"
    },
